{
  "name": "S3 Backup",
  "version": "0.0.1",
  "slug": "s3-backup",
  "description": "Sync Backups to your S3 bucket",
  "url": "https://github.com/geraldhansen/homeassistant-addons/tree/main/s3-backup",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "boot": "manual",
  "init": false,
  "startup": "once",
  "advanced": true,
  "hassio_api": true,
  "hassio_role": "backup",
  "options": {
    "aws_access_key": "",
    "aws_secret_access_key": "",
    "bucket_name": "",
    "bucket_region": "eu-central-1",
    "endpoint_url": "",
    "delete_local_backups": true,
    "local_backups_to_keep": 3
  },
  "schema": {
    "aws_access_key": "str",
    "aws_secret_access_key": "password",
    "bucket_name": "str",
    "bucket_region": "str",
    "endpoint_url": "str",
    "delete_local_backups": "bool",
    "local_backups_to_keep": "int"
  },
  "map": [
    "backup:rw"
  ]
}